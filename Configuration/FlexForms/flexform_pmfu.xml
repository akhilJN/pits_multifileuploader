			<ROOT>
				<TCEforms>
					<sheetTitle>Initail Conf</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexform.main.form>
						<TCEforms>
							<label>Select Extension to add PMFU</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
							            <numIndex index="0"></numIndex>
							            <numIndex index="1"></numIndex>
							        </numIndex>
							        <numIndex index="1" type="array">
							            <numIndex index="0">1111</numIndex>
							            <numIndex index="1">1</numIndex>
							        </numIndex>
							        <numIndex index="2" type="array">
							            <numIndex index="0">2222</numIndex>
							            <numIndex index="1">2</numIndex>
							        </numIndex>
							        <numIndex index="3" type="array">
							            <numIndex index="0">3333</numIndex>
							            <numIndex index="1">3</numIndex>
							        </numIndex>
							        <numIndex index="4" type="array">
							            <numIndex index="0">4444</numIndex>
							            <numIndex index="1">4</numIndex>
							        </numIndex>
							        <numIndex index="5" type="array">
							        	<numIndex index="0">5555</numIndex>
							            <numIndex index="1">250</numIndex>
							        </numIndex>
								</items>
								<size>1</size>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.flexform.main.form>
					<settings.field.required>
						<TCEforms>
							<label>Max Number of Blogs</label>
							<config> 
								<type>input</type>
								<size>2</size>
								<eval>int</eval>
								<default>10</default>
							</config>
						</TCEforms>
					</settings.field.required>
				</el>
			</ROOT>
		</main>
	</sheets>
</T3DataStructure>